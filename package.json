{
  "name": "riderr-starterkit",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "build-all": "npm install --pure-lockfile && npm run build && npm run export && npx cap copy",
    "dev": "npm run build-all && next dev",
    "open-ios": "npm run build-all && npx cap open ios",
    "open-android": "npm run build-all && npx cap open android",
    "open-both": "npm run build-all && npx cap open android && npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^3.0.1",
    "@capacitor/cli": "^3.0.1",
    "@capacitor/core": "^3.0.1",
    "@capacitor/ios": "^3.0.1",
    "@capacitor/status-bar": "^1.0.1",
    "@ionic/react": "^5.5.2",
    "@ionic/react-router": "^5.5.2",
    "autoprefixer": "^10.2.6",
    "classnames": "^2.2.6",
    "next": "^11.0.0",
    "postcss": "^8.3.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-virtuoso": "^1.1.1",
    "tailwindcss": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "ionicons": "^5.2.3",
    "prettier": "^2.2.1",
    "pullstate": "^1.20.5",
    "react-use-gesture": "^9.1.3",
    "reselect": "^4.0.0"
  }
}
